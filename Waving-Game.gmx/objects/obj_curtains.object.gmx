            <string>switch(curtainsState){
    case 0 : 
        curtainsState = 1;
        break;
    case 1 :
        if (curtainleft &gt;= -room_width/2){
            curtainleft -= cspeed;
            curtainright += cspeed;
        }
        else{
            curtainsState = 2;
        }
        break;
    case 2 : if(global.curtains == 1){curtainsState = 3} break;
    case 3 : 
        if (curtainleft &lt;= 0){
            curtainleft += cspeed;
            curtainright -= cspeed;
        }
        else{
            global.curtains = 0;
            curtainsState = 4;               
        }
        break;
    case 4: 
        switch(room_get_name(room)){
            case "room0" : room_goto(room1); break;
            case "room1" : room_goto(room2); break;
            case "room2" : room_goto(room3); break;
            case "room3" : room_goto(room0); break;
        }
        curtainsState = 0;
        break;
}

if (room_get_name(room) == menu){
